{{> header}}
<div class="content">
    {{#if user.isAuthenticated}} 
        {{> navigation}} 
    {{/if}}
    <section id="viewCatalog">
        <div class="posts">
            {{#each posts}}
                {{> post}}
            {{else}}
                <p style="font-style: italic">No posts in database</p>
            {{/each}}
        </div>
    </section>
</div>
{{> footer}}